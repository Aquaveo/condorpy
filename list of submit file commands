#PRE AND POST SCRIPTS IMPLEMENTED WITH SPECIALLY-NAMED ATTRIBUTES

+PreCmd = <TT>"</TT>< executable ><TT>"</TT>
+PreArgs = <TT>"</TT>< argument_list ><TT>"</TT>
+PreArguments = <TT>"</TT>< argument_list ><TT>"</TT>
+PostCmd = <TT>"</TT>< executable ><TT>"</TT>
+PostArgs = <TT>"</TT>< argument_list ><TT>"</TT>
+PostArguments = <TT>"</TT>< argument_list ><TT>"</TT>
